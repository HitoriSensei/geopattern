{
	"name": "@victr/geopattern",
	"version": "1.2.8",
	"description": "Generate beautiful SVG patterns",
	"keywords": [
		"svg",
		"pattern",
		"geometric",
		"background"
	],
	"homepage": "https://github.com/victrme/geopattern/",
	"bugs": "https://github.com/victrme/geopattern/issues",
	"license": "MIT",
	"contributors": [
		"Brandon Mills",
		"Jason Long"
	],
	"files": [
		"dist/",
		"README.md"
	],
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/geopattern.d.ts",
				"default": "./dist/geopattern.esm.js"
			},
			"require": {
				"default": "./dist/geopattern.cjs.js"
			}
		}
	},
	"main": "./dist/geopattern.cjs.js",
	"types": "./dist/geopattern.d.ts",
	"scripts": {
		"test": "mocha tests",
		"build": "node ./build.config.js && npm run types",
		"types": "npx -p typescript tsc ./dist/geopattern.esm.js --outFile ./dist/geopattern.d.ts --declaration --allowJs --emitDeclarationOnly"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/victrme/geopattern"
	},
	"devDependencies": {
		"esbuild": "^0.19.2",
		"mocha": "^10.2.0",
		"svg-parser": "^2.0.4"
	}
}
